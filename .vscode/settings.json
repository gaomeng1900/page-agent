{
	"editor.fontLigatures": true,
	"cSpell.words": [
		"HITL",
		"innerhtml",
		"llms",
		"opensource",
		"retryable",
		"wouter"
	],
	"markdownlint.config": {
		// "comment": "Relaxed rules",
		"default": true,
		"whitespace": false,
		"line_length": false,
		"ul-indent": false,
		"no-inline-html": false,
		"no-bare-urls": false,
		"fenced-code-language": false,
		"first-line-h1": false,
		"block-spacing": false,
		"blanks-around-lists": false,
		"ol-prefix": false
	}
}
